{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React Stater kit with RxJS, Webpack 2, React Router v4 and Babel",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel dev lint:watch",
    "dev": "babel-node devServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* devServer.js config.js client",
    "lint:watch": "npm run lint -- --watch",
    "mockserver": "json-server --watch mock-server/db.json --port 5001"
  },
