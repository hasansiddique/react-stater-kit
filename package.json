    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "redux-thunk": "2.0.1",
    "rxjs": "^5.5.2"
  },
